  
 int led=12;

void setup() {

// put your setup code here, to run once:

pinMode(led, OUTPUT);

}

void loop() {

// put your main code here, to run repeatedly:

digitalWrite(led, HIGH);//it will send data as 1 to turn on the led
delay(1000);

digitalWrite(led, LOW);//it will be sending data as 0 to turn off the led
delay(1000);
}
